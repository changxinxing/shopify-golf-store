  justify-content: flex-end;
  margin: 20px;
  
  @include media-query($medium-up) {
       min-height: 100vh;
       flex-direction: row;
      padding: 20px;
  }
  
  align-items: center;
  @include media-query($small) {
     background: #f7f7f7 !important;
  }

  
}

.dj-page-product-row-container h2, .dj-page-product-row-container h4, .dj-page-product-row-container h5{
  font-family: $hero-style, sans-serif;
}

.dj-page-show-image-mobile {
 	display: none;
  
  @include media-query($small) {
     display: block;
  }
  
}

.dj-page-product-row-container .djpage-content-container {
  background-color: #f7f7f7;
       padding: 20px;
  border-radius: 10px;
  width: 100%;
    
   @include media-query($medium-up) {
     width: 50%; 
   }
    
}

.djpage-video-row-container {

  width: 100%;
    
   @include media-query($medium-up) {
     width: 60%; 
   }
    
}

.dj-page-product-img-container {
    width: 100%;
    
   @include media-query($medium-up) {
     width: 60%; 
   }
}

.djpage-content-container .copy {
  color: $color-text;
}

.djpage-content-container .quote {
  font-style: italic;
}

.djpage-content-container h2 {
  color: $color-text;
  font-weight: 700;
}

.djpage-content-container h4 {
  font-weight: 300;
  color: $color-brand;
  margin: 0 0 5px;
  text-transform: uppercase;
  font-weight: 700;
}


.djpage-header-image-column {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  background: #111;
  @include media-query($medium-up) {
    width: 60%;
    background: none;
  }
  
  @include media-query($widescreen) {
    width: 50%;
  }
}

.djpage-header-image-column .dj-headshot {
  max-width: 100%;
  object-fit: cover;
  object-position: left;
  align-self: flex-end;
  width: 100%;
}

.djpage-header-image-column .djsignature {
  width: 175px;
  position: absolute;
  @include media-query($medium-up) {
      width: 300px;
   }
  
}

.djpage-header-content-column {
  padding: 20px;
  color: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  
    @include media-query($medium-up) {
      width: 40%;
   }
}

.djpage-header-content-column p {
  color: #fff;
}

.djpage-header-container h1 {
	color: #fff;
    font-weight: 700;
  	
  @include media-query($medium-up) {
        font-size: 3em;
   }
}

.djpage-header-container {
  display: flex;
  color: #fff;
  background: url('https://cdn.shopify.com/s/files/1/2642/5702/files/header_back_7efa8bec-1a30-4ef4-86d9-2bce2192fbcf.jpg?v=1615555880');
  min-height: 100vh;
  flex-direction: column;
  @include media-query($medium-up) {
    flex-direction: row;
   }
  
}

.quote-video {
	display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    text-align: center;
    color: #ffffff;
    align-items: center;
  	position: relative;
    width: 100%;
    height: 100%;
}

.quote-video video {
  	width: 100%;
}

.video-overlay-container {
  	position: absolute;
    word-break: break-word;
    color: #ffffff;
    width: 100%;
  	z-index: 3;
}

.video-overlay-dark {
  	height: 100%;
    width: 100%;
    position: absolute;
    top: 0px;
    left: 0px;
    z-index: 2;
    background: #000;
    opacity: 0.3;
}

.quote-novideo {
	display: flex;
    flex-flow: column;
    justify-content: center;
    text-align: center;
    color: #ffffff;
    align-items: center;
  	position: relative;
    width: 100%;
    height: 100%;
  	background: $color-brand;
   	padding: 30px;
   	@include media-query($medium-up) {
    	padding: 10%;
  	}
}

.quote-overlay-container {

  	word-break: break-word;
  	color: #ffffff;
  	width: 100%;
  	padding: 30px;
  	@include media-query($medium-up) {
    	padding: 10%;
  	}
}

.quote-author-img {
 	width: 100px;
  	border-radius: 50px;
}

.djpage-button-row {
  display: flex;
  justify-content: center;
  
  @include media-query($medium-up) {
    justify-content: flex-start;
  }
}

.djpage-learnmore-btn {
  	background-color: transparent;
    color: $color-brand;
    border: 1px solid $color-brand;
}

.djbutton {
  min-height: 40px;
  margin-right: 10px;
  padding: 8px 15px;
  text-transform: uppercase;
}

.djpage-feature-column {
  padding: 10px;
  text-align: center;
  width: calc(33%-20);
}

.djpage-feature-column h5 {
  font-weight: 700;
  text-transform: uppercase;
}

.djpage-product-features-container {
  display: flex;
  align-items: center;
  justify-content: space-evenly;
  margin-bottom: 10px;
  margin-top: -5px;
  
  @include media-query($medium-up) {
    justify-content: flex-start;
  }
}

.product-feature-icon-image {
  max-width: 100%;
   
  @include media-query($medium-up) {
    max-width: 125px;
  }
}

.djpage-collection-row {
   margin-top: 30px;
}

.djpage-collection-row .page-width{
     display: flex;
  	flex-direction: column;
  @include media-query($medium-up) {
   flex-direction: row;
  }
}

.djpage-image-column {
   width: 100%;
 	@include media-query($medium-up) {
    width: 60%
  }
}
  

.djpage-collection-promo-col {
  width: 100%;
  @include media-query($medium-up) {
    width: 40%
  }
  	padding: 20px;
}

.djpage-reverse {
  @include media-query($medium-up) {
    flex-direction: row-reverse;
  }
  
  
}

.djpage-collection-row ul li, .djpage-content-container ul li {
  list-style: initial;
}

.djpage-collection-row .price-item--regular {
 text-decoration: line-through;
    margin-right: 10px;
}

.djpage-collection-row .price-item--sale {
 color: #36CB64; 
}

.djpage-collection-row ul, .djpage-content-container ul {
  display: block;
    list-style-type: disc;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 40px;
}

.djpage-price-row {
 	display: flex;
  	flex-direction: row;
}

.djpage-learnmore-row {
  text-align: center;
  padding: 30px;
  background-color: #3a3a3a;
  margin-top: 30px;
  color: #fff;
  font-weight: 700;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.djpage-learnmore-row img {
  width: 50px;
  @include media-query($medium-up) {
    width: 100px;
  }
}

.link-color {
  color: #1ed761;
  transition: color 200ms ease-in-out;
}

.mobile-fit-video {
 width: 100% !important;
  object-fit: cover;
}

.embed-container { position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden; max-width: 100%; margin-bottom: 20px; } 
.embed-container iframe, .embed-container object, .embed-container embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }

